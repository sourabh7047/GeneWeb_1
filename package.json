{
  "name": "genomeweb-backend",
  "version": "5.9.1",
  "description": "\"backend of genome project\"",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cd project-geneweb && npm run build",
    "install-client": "cd project-geneweb && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "start": "node server.js",
    "client": "cd project-geneweb && npm start",
    "dev": "concurrently -n 'server,client' -c 'red,green'  \"nodemon server.js\" \"npm run client\""
  },
  "author": "\"sourabh asharma\"",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "colors": "^1.4.0",
    "concurrently": "^7.3.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "node": "^14.17.3",
    "node-fetch": "^3.2.10",
    "xhr2": "^0.2.1",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
